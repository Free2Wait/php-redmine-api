{
    "name": "kbsali/redmine-api",
    "type": "library",
    "description": "Redmine API client",
    "homepage": "https://github.com/kbsali/php-redmine-api",
    "keywords": ["redmine", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Saliou",
            "email": "kevin@saliou.name",
            "homepage": "http://kevin.saliou.name"
        }
    ],
    "require": {
        "php": "^5.4 || ^7.0",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "ext-json": "*",
        "free2wait/composer-free2wait": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^4.8.35 || ^5.4.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Redmine\\": "lib/Redmine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redmine\\Tests\\": "tests/"
        }
    },
    "free2wait": {
        "to": {
            "paypal": "your.paypal.to.receive.payouts@example.com"
        }
    }
}
